                // Slider
                #images-container
                    .slide
                        img(src='img/slider/slider_indiv.jpg', alt='Thérapie individuelle')
                        span.slide-caption Thérapie individuelle
                    .slide
                        img(src='img/slider/slider_couple.jpg', alt='Thérapie de couple')
                        span.slide-caption Thérapie de couple
                    .slide
                        img(src='img/slider/slider_trans.jpg', alt='Santé transsexuelle')
